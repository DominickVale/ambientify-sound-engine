{
  "name": "ambientify-sound-engine",
  "version": "0.0.1",
  "description": "Ambientify sound engine module",
  "main": "./src/index.ts",
  "source": "src/index",
  "private": "false",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "ambientify-sound-engine.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "bootstrap": "yarn"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/DominickVale/ambientify-sound-engine",
  "author": "DominickVale (https://github.com/DominickVale)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DominickVale/ambientify-sound-engine/issues"
  },
  "homepage": "https://github.com/DominickVale/ambientify-sound-engine#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@react-native/eslint-config": "^0.73.1",
    "@types/jest": "^26.0.0",
    "@types/react": "18.2.6",
    "@types/react-native": "0.73.0",
    "commitlint": "^11.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-native": "0.73.1",
    "react-native-builder-bob": "^0.35.0",
    "release-it": "^14.2.2",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "0.73.1"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {},
  "engines": {
    "node": ">=18"
  }
}
